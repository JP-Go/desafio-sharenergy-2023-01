generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Client{
  id       String  @id @default(uuid())
  name     String
  email    String  @unique
  phone    String
  address  String
  cpf      String  @unique
}

model Address{
  city     String
  cep      String
  state    String
  street   String
  number   String
}
